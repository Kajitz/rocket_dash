.box.box-default
  .box-header.with-border
    %h4.box-title
      #{Admin.model_name.human}情報
    .box-tools
      = link_to '一覧', admin_admins_path, class: 'btn btn-default'
      = link_to '編集', edit_admin_admin_path(@admin), class: 'btn btn-primary'
  .box-body
    %table.table
      %tr
        %th= human(Admin, :name)
        %td= @admin.name
      %tr
        %th= human(Admin, :email)
        %td= @admin.email
